{
  "name": "error-face",
  "displayName": "Error Face and Explain",
  "description": "In Your Face, shows you Doom 'Ouch Faces' that correlate to the number of errors in your code!",
  "version": "1.1.2",
  "publisher": "naveensh7",
  "homepage": "https://github.com/NaveenSh7",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/NaveenSh7/VSCode_Extn_Err"
  },
 "engines": {
  "vscode": "^1.96.0"
}
,
  "categories": [
    "Visualization"
  ],
  "icon": "assets/icon.png",
  "galleryBanner": {
    "color": "#191919",
    "theme": "dark"
  },
  "keywords": [
    "your face",
    "visualization",
    "Pikachu",
    "Cat",
    "face",
    "errors",
    "error lens",
    "error count",
    "funny",
    "meme"
  ],
  "__metadata": {
    "publisherDisplayName": "Naveen S H"
  },
  "activationEvents": [
   
  ],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "custom-activitybar",
          "title": "In Your Face",
          "icon": "assets/logo_bito.svg"
        }
      ]
    },
    "views": {
      "custom-activitybar": [
        {
          "type": "webview",
          "id": "in-your-face.openview",
          "name": "In Your Face",
          "contextualTitle": "In Your Face"
        }
      ]
    },
    "configuration": {
      "title": "InYourFace",
      "properties": {
        "InYourFace.error.usewarnings": {
          "type": "boolean",
          "default": false,
          "description": "Should take in account warnings"
        }
      }
    },
    "commands": [
      {
        "command": "in-your-face.openview.focus",
        "category": "In Your Face",
        "title": "Open Sidebar",
        "icon": "$(clear-all)"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.4",
    "@types/mocha": "^9.0.0",
    "@types/node": "^14.18.63",
    "@types/vscode": "^1.96.0",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "@vscode/test-electron": "^1.6.2",
    "eslint": "^7.32.0",
    "glob": "^7.1.7",
    "mocha": "^9.1.1",
    "typescript": "^4.4.3"
  }
}
